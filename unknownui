                                    bB.Visible = false
                                end
                                function bR:Show()
                                    bB.Visible = true
                                    return bR
                                end
                                function ShowSwitch()
                                    bB.Visible = true
                                end
                                function bR:Delete()
                                    bB:Destroy()
                                    return bR
                                end
