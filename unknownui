                    function()
                        wait(0.12)
                        for K, w in next, Z:GetDescendants() do
                            if w:IsA("Frame") then
                                t:Animate(w, {BackgroundTransparency = 1}, "Exit Animation")
                                t:Animate(w, {Size = UDim2.new(0, 0, 0, 0)}, "Exit Animation")
                            end
                            if w:IsA("TextLabel") then
                                t:Animate(w, {BackgroundTransparency = 1}, "Exit Animation")
                                t:Animate(w, {TextTransparency = 1}, "Exit Animation")
                                t:Animate(w, {Size = UDim2.new(0, 0, 0, 0)}, "Exit Animation")
                            end
                            if w:IsA("TextButton") then
                                t:Animate(w, {BackgroundTransparency = 1}, "Exit Animation")
                                t:Animate(w, {TextTransparency = 1}, "Exit Animation")
                                t:Animate(w, {Size = UDim2.new(0, 0, 0, 0)}, "Exit Animation")
                            end
                            if w:IsA("ImageButton") then
                                t:Animate(w, {BackgroundTransparency = 1}, "Exit Animation")
                                t:Animate(w, {ImageTransparency = 1}, "Exit Animation")
                                t:Animate(w, {Size = UDim2.new(0, 0, 0, 0)}, "Exit Animation")
                            end
                            if w:IsA("ImageLabel") then
                                t:Animate(w, {BackgroundTransparency = 1}, "Exit Animation")
                                t:Animate(w, {ImageTransparency = 1}, "Exit Animation")
                                t:Animate(w, {Size = UDim2.new(0, 0, 0, 0)}, "Exit Animation")
                            end
                            coroutine.wrap(
                                function()
                                    wait(0.18)
                                    s:Delete()
                                end
                            )
                        end
                    end
                )
