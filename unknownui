                                bN.TextSize = 14.000
                                t:Create(
                                    {
                                        Name = "Switch Animation",
                                        Speed = 0.14,
                                        Style = Enum.EasingStyle.Sine,
                                        Direction = Enum.EasingDirection.InOut,
                                        Loop = 0,
                                        Reverse = false,
                                        Delay = 0
                                    }
                                )
                                if bA.State == true then
                                    if bA.Disabled == false then
                                        bJ.BackgroundColor3 = Color3.fromRGB(190, 190, 190)
                                        bJ.Position = UDim2.new(0, 24, 0.5, 0)
                                        if bA.Warning == true then
                                            bH.BackgroundColor3 = Color3.fromRGB(177, 73, 75)
                                            bH.BackgroundTransparency = 0
                                        else
                                            bH.BackgroundColor3 = config.Accent
                                            bH.BackgroundTransparency = 0
                                        end
                                    else
                                        bJ.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
                                        bJ.Position = UDim2.new(0, 10, 0.5, 0)
                                        if bA.Warning == true then
                                            bH.BackgroundColor3 = Color3.fromRGB(141, 54, 56)
                                            bH.BackgroundTransparency = 0.2
                                        else
                                            bH.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
                                            bH.BackgroundTransparency = 0.2
                                        end
                                    end
                                end
                                if bA.Disabled == true then
                                    bB.BackgroundTransparency = 0.2
                                    if bA.Warning == true then
                                        bE.TextColor3 = Color3.fromRGB(141, 54, 56)
                                    else
                                        bE.TextColor3 = Color3.fromRGB(100, 100, 100)
                                    end
                                else
                                    bB.BackgroundTransparency = 0
                                    if bA.Warning == true then
                                        bE.TextColor3 = Color3.fromRGB(177, 73, 75)
                                    else
                                        bE.TextColor3 = Color3.fromRGB(149, 149, 149)
                                    end
                                end
