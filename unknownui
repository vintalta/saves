                    function b4:ChangeHeader(bz, bool)
                        bz = bz or aZ.Text
                        aZ.Text = bz
                        if bool then
                            t:Animate(b1, {TextTransparency = 1}, "Text Change Animation")
                            wait(0.12)
                            b1.Text = aZ.Text
                            wait()
                            t:Animate(b1, {TextTransparency = 0}, "Text Change Animation")
                        else
                            b1.Text = aZ.Text
                        end
                        return b4
                    end
                    function ChangeSectionHeader(bz, bool)
                        bz = bz or aZ.Text
                        aZ.Text = bz
                        if bool then
                            t:Animate(b1, {TextTransparency = 1}, "Text Change Animation")
                            wait(0.12)
                            b1.Text = aZ.Text
                            wait()
                            t:Animate(b1, {TextTransparency = 0}, "Text Change Animation")
                        else
                            b1.Text = aZ.Text
                        end
                    end
                    function b4:ChangeAlignment(bz, bool)
                        bool = bool or true
                        bz = bz or "left"
                        aZ.Alignment = bz
                        if bool then
                            if bz == "left" then
                                t:Animate(b1, {TextTransparency = 1}, "Text Change Animation")
                                wait(0.12)
                                b1.TextXAlignment = Enum.TextXAlignment.Left
                                b2.PaddingRight = UDim.new(0, 0)
                                b2.PaddingLeft = UDim.new(0, 15)
                                wait()
                                t:Animate(b1, {TextTransparency = 0}, "Text Change Animation")
                            elseif bz == "right" then
                                t:Animate(b1, {TextTransparency = 1}, "Text Change Animation")
                                wait(0.12)
                                b1.TextXAlignment = Enum.TextXAlignment.Right
                                b2.PaddingRight = UDim.new(0, 15)
                                b2.PaddingLeft = UDim.new(0, 0)
                                wait()
                                t:Animate(b1, {TextTransparency = 0}, "Text Change Animation")
                            elseif bz == "center" then
                                t:Animate(b1, {TextTransparency = 1}, "Text Change Animation")
                                wait(0.12)
                                b1.TextXAlignment = Enum.TextXAlignment.Center
                                b2.PaddingRight = UDim.new(0, 0)
                                b2.PaddingLeft = UDim.new(0, 0)
                                wait()
                                t:Animate(b1, {TextTransparency = 0}, "Text Change Animation")
                            end
                        else
                            if bz == "left" then
                                b1.TextXAlignment = Enum.TextXAlignment.Left
                                b2.PaddingRight = UDim.new(0, 0)
                                b2.PaddingLeft = UDim.new(0, 15)
                            elseif bz == "right" then
                                b1.TextXAlignment = Enum.TextXAlignment.Right
                                b2.PaddingRight = UDim.new(0, 15)
                                b2.PaddingLeft = UDim.new(0, 0)
                            elseif bz == "center" then
                                b1.TextXAlignment = Enum.TextXAlignment.Center
                                b2.PaddingRight = UDim.new(0, 0)
                                b2.PaddingLeft = UDim.new(0, 0)
                            end
                        end
                        return b4
                    end
                    function ChangeAlignment(bz, bool)
                        bool = bool or true
                        bz = bz or "left"
                        aZ.Alignment = bz
                        if bool then
                            if bz == "left" then
                                t:Animate(b1, {TextTransparency = 1}, "Text Change Animation")
                                wait(0.12)
                                b1.TextXAlignment = Enum.TextXAlignment.Left
                                b2.PaddingRight = UDim.new(0, 0)
                                b2.PaddingLeft = UDim.new(0, 15)
                                wait()
                                t:Animate(b1, {TextTransparency = 0}, "Text Change Animation")
                            elseif bz == "right" then
                                t:Animate(b1, {TextTransparency = 1}, "Text Change Animation")
                                wait(0.12)
                                b1.TextXAlignment = Enum.TextXAlignment.Right
                                b2.PaddingRight = UDim.new(0, 15)
                                b2.PaddingLeft = UDim.new(0, 0)
                                wait()
                                t:Animate(b1, {TextTransparency = 0}, "Text Change Animation")
                            elseif bz == "center" then
                                t:Animate(b1, {TextTransparency = 1}, "Text Change Animation")
                                wait(0.12)
                                b1.TextXAlignment = Enum.TextXAlignment.Center
                                b2.PaddingRight = UDim.new(0, 0)
                                b2.PaddingLeft = UDim.new(0, 0)
                                wait()
                                t:Animate(b1, {TextTransparency = 0}, "Text Change Animation")
                            end
                        else
                            if bz == "left" then
                                b1.TextXAlignment = Enum.TextXAlignment.Left
                                b2.PaddingRight = UDim.new(0, 0)
                                b2.PaddingLeft = UDim.new(0, 15)
                            elseif bz == "right" then
                                b1.TextXAlignment = Enum.TextXAlignment.Right
                                b2.PaddingRight = UDim.new(0, 15)
                                b2.PaddingLeft = UDim.new(0, 0)
                            elseif bz == "center" then
                                b1.TextXAlignment = Enum.TextXAlignment.Center
                                b2.PaddingRight = UDim.new(0, 0)
                                b2.PaddingLeft = UDim.new(0, 0)
                            end
                        end
                    end
                    function b4:ChangeCallback(bz)
                        bz = bz or function()
                            end
                        aZ.Callback = bz
                        return b4
                    end
                    function b4:Hide()
                        a_.Visible = false
                        return b4
                    end
                    function HideSection()
                        a_.Visible = false
                    end
                    function b4:Show()
                        a_.Visible = true
                        return b4
                    end
                    function ShowSection()
                        a_.Visible = true
                    end
                    function b4:Delete()
                        a_:Destroy()
                        return b4
                    end
                    function DeleteSection()
                        a_:Destroy()
                    end
                    aZ.Callback()
                    return b4
                end
                function aY:ChangeHeader(bz, bool)
                    bool = bool or true
                    bz = bz or u.Header
                    u.Header = bz
                    if bool then
                        t:Animate(aQ, {TextTransparency = 1}, "Text Change Animation")
                        wait(0.12)
                        aQ.Text = u.Header
                        wait()
                        t:Animate(aQ, {TextTransparency = 0}, "Text Change Animation")
                    else
                        aQ.Text = u.Header
                    end
                    return aY
                end
                function ChangeHeader(bz)
                    bool = bool or true
                    bz = bz or u.Header
                    u.Header = bz
                    if bool then
                        t:Animate(aQ, {TextTransparency = 1}, "Text Change Animation")
                        wait(0.12)
                        aQ.Text = u.Header
