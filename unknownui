                end
                function aY:ChangeSize(c0, c1)
                    c0 = c0 or Y.Sizes.X
                    c1 = c1 or Y.Sizes.Y
                    t:Animate(_, {Size = UDim2.new(0, c0, 0, c1)}, "Size Change Animation")
                    return aY
                end
                function ChangeSize(c0, c1)
                    c0 = c0 or Y.Sizes.X
                    c1 = c1 or Y.Sizes.Y
                    Y.Sizes.X = c0
                    Y.Sizes.Y = c1
                    t:Animate(_, {Size = UDim2.new(0, c0, 0, c1)}, "Size Change Animation")
                end
                function aY:ReCentre()
                    t:Animate(_, {Position = UDim2.new(0.5, 0, 0.5, 0)}, "Position Change Animation")
                    return aY
                end
                function ReCentre()
                    t:Animate(_, {Position = UDim2.new(0.5, 0, 0.5, 0)}, "Position Change Animation")
                end
                function aY:ChangePosition(c0, c1)
                    c0 = c0 or Y.Sizes.X
                    c1 = c1 or Y.Sizes.Y
                    Y.Sizes.X = c0
                    Y.Sizes.Y = c1
                    t:Animate(_, {Position = UDim2.new(c0, 0, c1, 0)}, "Position Change Animation")
                    return aY
                end
                function ChangePosition(c0, c1)
                    c0 = c0 or Y.Sizes.X
                    c1 = c1 or Y.Sizes.Y
                    Y.Sizes.X = c0
                    Y.Sizes.Y = c1
                    t:Animate(_, {Position = UDim2.new(c0, 0, c1, 0)}, "Position Change Animation")
                end
                function aY:AttachPosition(c2)
                    c2 = c2 or "center"
                    if c2 == "centre" then
                        t:Animate(_, {Position = UDim2.new(0.5, 0, 0.5, 0)}, "Position Change Animation")
                    elseif c2 == "left" then
                        t:Animate(_, {Position = UDim2.new(0.1, 0, 0.5, 0)}, "Position Change Animation")
                    elseif c2 == "top left" then
                        t:Animate(_, {Position = UDim2.new(0.1, 0, 0.23, 0)}, "Position Change Animation")
                    elseif c2 == "bottom left" then
                        t:Animate(_, {Position = UDim2.new(0.1, 0, 0.77, 0)}, "Position Change Animation")
                    elseif c2 == "top center" then
                        t:Animate(_, {Position = UDim2.new(0.5, 0, 0.77, 0)}, "Position Change Animation")
                    elseif c2 == "bottom center" then
                        t:Animate(_, {Position = UDim2.new(0.5, 0, 0.23, 0)}, "Position Change Animation")
                    elseif c2 == "right" then
                        t:Animate(_, {Position = UDim2.new(0.9, 0, 0.5, 0)}, "Position Change Animation")
                    elseif c2 == "top right" then
                        t:Animate(_, {Position = UDim2.new(0.9, 0, 0.23, 0)}, "Position Change Animation")
                    elseif c2 == "bottom right" then
                        t:Animate(_, {Position = UDim2.new(0.9, 0, 0.77, 0)}, "Position Change Animation")
                    end
                    return aY
                end
                function AttachPosition(c2)
                    c2 = c2 or "center"
                    if c2 == "center" then
                        t:Animate(_, {Position = UDim2.new(0.5, 0, 0.5, 0)}, "Position Change Animation")
                    elseif c2 == "left" then
                        t:Animate(_, {Position = UDim2.new(0.1, 0, 0.5, 0)}, "Position Change Animation")
                    elseif c2 == "top left" then
                        t:Animate(_, {Position = UDim2.new(0.1, 0, 0.23, 0)}, "Position Change Animation")
                    elseif c2 == "bottom left" then
                        t:Animate(_, {Position = UDim2.new(0.1, 0, 0.77, 0)}, "Position Change Animation")
                    elseif c2 == "top center" then
                        t:Animate(_, {Position = UDim2.new(0.5, 0, 0.77, 0)}, "Position Change Animation")
                    elseif c2 == "bottom center" then
                        t:Animate(_, {Position = UDim2.new(0.5, 0, 0.23, 0)}, "Position Change Animation")
                    elseif c2 == "right" then
                        t:Animate(_, {Position = UDim2.new(0.9, 0, 0.5, 0)}, "Position Change Animation")
                    elseif c2 == "top right" then
                        t:Animate(_, {Position = UDim2.new(0.9, 0, 0.23, 0)}, "Position Change Animation")
                    elseif c2 == "bottom right" then
                        t:Animate(_, {Position = UDim2.new(0.9, 0, 0.77, 0)}, "Position Change Animation")
                    end
                end
                function aY:Delete()
                    s:Delete()
                    return aY
                end
                function DeleteMenu()
                    s:Delete()
                end
                function aY:Hide()
                    s:Toggle(false)
                    return aY
                end
                function HideMenu()
                    s:Toggle(false)
                end
                function aY:Show()
                    s:Toggle(true)
                    return aY
                end
                function ShowMenu()
                    s:Toggle(true)
                end
                function aY:ChangeLatency(bz)
                    bz = bz or Y.Latency
                    getgenv().Latency = bz
                    return aY
                end
                function ChangeLatency(bz)
                    bz = bz or Y.Latency
                    getgenv().Latency = bz
                end
                function aY:ChangeCallback(bz)
                    bz = bz or function()
                        end
                    Y.Callback = bz
                    return aY
                end
                Y.Callback()
                return aY
            end
        end
        if X == "notifications" and W.UI.Notifications == false then
            W.UI.Notifications = true
        end
        if X == "watermark" and W.UI.Watermark == false then
            W.UI.Watermark = true
        end
        if X == "whitelist" and W.UI.Whitelist == false then
            W.UI.Whitelist = true
        end
        if X == "introduction" and W.UI.Introduction == false then
            W.UI.Introduction = true
        end
    end
