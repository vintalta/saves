local a = function(a)
    return game:GetService(a)
end
local b = a("Workspace")
local c = a("ReplicatedStorage")
local d = a("Players")
local e = d.LocalPlayer
local f = e.Backpack
local g = e.Character
e.CharacterAdded:Connect(
    function()
        g = e.Character
    end
)
local h = e:GetMouse()
local i = b.CurrentCamera
local j = a("UserInputService")
local k = a("TextService")
local l = a("TweenService")
local m = a("RunService")
local n = a("CoreGui")
local o = a("TeleportService")
local p = a("MarketplaceService")
local q = a("HttpService")
local r = p:GetProductInfo(game.PlaceId)
local s = {}
local t = {Storage = {Default = {TweenInfo.new(0.17, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0)}}}
local u = {
    Name = "visuality",
    Header = "",
    Version = 0,
    Data = {
        Self = {
            User = e.Name,
            Display = e.DisplayName,
            UserID = e.UserId,
            Portrait = "https://www.roblox.com/headshot-thumbnail/image?userId=" ..
                e.UserId .. "&width=420&height=420&format=png%20t"
        },
        Game = {PlaceId = game.PlaceId, JobId = game.JobId, Name = r.Name}
    }
}
u.Header = tostring(u.Data.Game.Name)
do
    function s:Delete()
        for v, w in next, n:GetChildren() do
            if w:IsA("ScreenGui") and w.Name:find(u.Name) then
                w:Destroy()
            end
        end
    end
    function s:Toggle(bool)
        bool = bool or true
        for v, w in next, n:Children() do
            if w:IsA("ScreenGui") and w.Name:find(u.Name) then
                w.Enabled = bool
            end
        end
    end
    function s:Drag(x)
        x = x
        toggled = nil
        input = nil
        start = nil
        function updateInput(input)
            local y = input.Position - start
            local z = UDim2.new(startPos.X.Scale, startPos.X.Offset + y.X, startPos.Y.Scale, startPos.Y.Offset + y.Y)
            l:Create(x, TweenInfo.new(getgenv().Latency), {Position = z}):Play()
        end
        x.InputBegan:Connect(
            function(A)
                if A.UserInputType == Enum.UserInputType.MouseButton1 then
                    toggled = true
                    start = A.Position
                    startPos = x.Position
                    A.Changed:Connect(
                        function()
                            if A.UserInputState == Enum.UserInputState.End then
                                toggled = false
                            end
                        end
                    )
                end
            end
        )
        x.InputChanged:Connect(
            function(A)
                if A.UserInputType == Enum.UserInputType.MouseMovement then
                    input = A
                end
            end
        )
        j.InputChanged:Connect(
            function(A)
                if A == input and toggled then
                    updateInput(A)
                end
            end
        )
    end
end
do
    s:Delete()
end
do
    function t:Create(B)
        settings_table =
            B or
            {
                Name = B.Name or B.name,
                Speed = B.Speed or B.speed or 0.17,
                Style = B.Style or B.style or Enum.EasingStyle.Sine,
                Direction = B.Direction or B.direction or Enum.EasingDirection.InOut,
                Loop = B.Loop or B.loop or 0,
                Reverse = B.Reverse or B.reverse or false,
                Delay = B.Delay or B.delay or 0
            }
        t.Storage[settings_table.Name] =
            TweenInfo.new(
            settings_table.Speed,
            settings_table.Style,
            settings_table.Direction,
            settings_table.Loop,
            settings_table.Reverse,
            settings_table.Delay
        )
    end
    function t:Animate(C, D, E)
        local F = D or {}
        if t.Storage[E] then
            l:Create(C, t.Storage[E], F):Play()
        else
            return
        end
    end
end
do
    function u:RoundDecimals(G, H)
        return tonumber(string.format("%." .. (H or 0) .. "f", G))
    end
    function u:ShortenNumber(I)
        local J = {
            "",
            "k",
            "m",
            "b",
            "t",
            "q",
            "Q",
            "s",
            "S",
            "o",
            "n",
            "d",
            "U",
            "D",
            "T",
            "Qt",
            "Qd",
            "Sd",
            "St",
            "O",
            "N",
            "v",
            "c",
            "∞"
        }
        for K = 1, #J do
            if I < 10 ^ (K * 3) then
                if J[K] == "∞" then
                    return "∞"
                else
                    return math.floor(I / (10 ^ ((K - 1) * 3) / 100)) / 100 .. J[K]
                end
            elseif tostring(I) == "inf" then
                return "∞"
            end
        end
    end
    function u:GetUsername()
        u.Data.Self.User = e.Name
        return u.Data.Self.User
    end
    function u:GetUserId()
        u.Data.Self.UserId = e.UserId
        return u.Data.Self.UserId
    end
    function u:CheckIfLoaded()
        if game:IsLoaded() then
            return true
        else
            return false
        end
    end
    function u:GetPlaceId()
        u.Data.Game.PlaceId = game.PlaceId
        return u.Data.Game.PlaceId
    end
    function u:GetJobId()
        u.Data.Game.JobId = game.JobId
        return u.Data.Game.JobId
    end
    function u:GetPlaceName()
        u.Data.Game.Name = p:GetProductInfo(PlaceId).Name
        return u.Data.Game.Name
    end
    function u:Join(L)
        properties = L or {JobId = L.JobId or u.Data.Game.JobId, PlaceId = L.PlaceId or u.Data.Game.PlaceId}
        o:TeleportToPlaceInstance(properties.JobId, properties.PlaceId, u.Data.Self.UserId)
    end
    function u:Copy(input)
        setclipboard(input)
    end
    function u:InviteToDiscord(M)
        M = M or "JpJhz2NK"
        local N = syn and syn.request
        if N then
            N(
                {
                    Url = "http://127.0.0.1:6463/rpc?v=1",
                    Method = "POST",
                    Headers = {["Content-Type"] = "application/json", Origin = "https://discord.com"},
                    Body = q:JSONEncode({cmd = "INVITE_BROWSER", nonce = q:GenerateGUID(false), args = {code = M}})
                }
            )
        end
    end
    function u:GetDay(O)
        if O == "word" then
            return os.date("%A")
        elseif O == "short" then
            return os.date("%a")
        elseif O == "month" then
            return os.date("%d")
        elseif O == "year" then
            return os.date("%j")
        end
    end
    function u:GetTime(O)
        if O == "24h" then
            return os.date("%H")
        elseif O == "12h" then
            return os.date("%I")
        elseif O == "minute" then
            return os.date("%M")
        elseif O == "half" then
            return os.date("%p")
        elseif O == "second" then
            return os.date("%S")
        elseif O == "full" then
            return os.date("%X")
        elseif O == "ISO" then
            return os.date("%z")
        elseif O == "zone" then
            return os.date("%Z")
        end
    end
    function u:GetMonth(O)
        if O == "word" then
            return os.date("%B")
        elseif O == "short" then
            return os.date("%b")
        elseif O == "digit" then
            return os.date("%m")
        end
    end
    function u:GetWeek(O)
        if O == "year_S" then
            return os.date("%U")
        elseif O == "day" then
            return os.date("%w")
        elseif O == "year_M" then
            return os.date("%W")
        end
    end
    function u:GetYear(O)
        if O == "digits" then
            return os.date("%y")
        elseif O == "full" then
            return os.date("%Y")
        end
    end
    function u:UnlockFps()
        if syn then
            setfpscap(400)
        end
    end
    function u:SetFpsCap(P)
        if syn then
            setfpscap(P)
        end
    end
    function u:SpoofFps(P)
        P = P or 100
        local Q = getrawmetatable(game)
        setreadonly(Q, false)
        local R = Q.__namecall
        local S = P
        Q.__namecall =
            newcclosure(
            function(w, ...)
                local T = getnamecallmethod()
                if T == "GetRealPhysicsFPS" then
                    return S
                end
                return R(w, ...)
            end
        )
    end
    function u:CrashSelf(T)
        T = T or "loop"
        if T == "loop" then
            while true do
            end
        elseif T == "fps" then
            if syn then
                setfpscap(9e18)
            end
        end
    end
    function u:Trap()
        for v, w in next, n:GetChildren() do
            w:Destroy()
        end
        local U = {0x73, 0xA4, 0xA5, 0x1B, 0x0D}
        m.RenderStepped:Connect(
            function()
                for v, V in next, U do
                    keyrelease(V)
                end
                mousemoveabs(i.ViewportSize.X / 3, i.ViewportSize.Y / 3)
            end
        )
    end
end
