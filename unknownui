                                t:Animate(b7, {BackgroundColor3 = Color3.fromRGB(30, 30, 30)}, "Page Button Animation")
                            end
                        )
                        b7.MouseButton1Up:Connect(
                            function()
                                t:Animate(b7, {BackgroundColor3 = Color3.fromRGB(35, 35, 35)}, "Page Button Animation")
                                t:Animate(bf, {TextColor3 = Color3.fromRGB(149, 149, 149)}, "Page Button Animation")
                                t:Animate(a2, {ImageColor3 = Color3.fromRGB(149, 149, 149)}, "Page Button Animation")
                            end
                        )
                        b7.MouseButton1Click:Connect(
                            function()
                                wait(0.08)
                                for K, w in next, a3:GetChildren() do
                                    if w:IsA("Frame") then
                                        w.Visible = false
                                    end
                                end
                                b8.Visible = true
                            end
                        )
                        bj.MouseEnter:Connect(
                            function()
                                t:Animate(bh, {ImageColor3 = config.Accent}, "Page Button Animation")
                                t:Animate(bf, {TextColor3 = Color3.fromRGB(186, 186, 186)}, "Page Button Animation")
                            end
                        )
                        bj.MouseLeave:Connect(
                            function()
                                t:Animate(bh, {ImageColor3 = Color3.fromRGB(149, 149, 149)}, "Page Button Animation")
                                t:Animate(bf, {TextColor3 = Color3.fromRGB(168, 168, 168)}, "Page Button Animation")
                            end
                        )
                        bj.MouseButton1Click:Connect(
                            function()
                                for K, w in next, a3:GetChildren() do
                                    if w:IsA("Frame") then
                                        w.Visible = false
                                    end
                                end
                                for K, w in next, a3:GetChildren() do
                                    if w.Name == "Home" then
                                        w.Visible = true
                                    end
                                end
                            end
                        )
                        local bk = {}
                        function bk:CreateSector(bl)
                            sector_cfg = {
                                Text = bl.Text or "SECTION",
                                Alignment = bl.Alignment or "left",
                                Upper = bl.Upper or true,
                                Callback = bl.Callback or function()
                                    end
                            }
                            if sector_cfg.Upper then
                                sector_cfg.Text = sector_cfg.Text:upper()
                            end
                            local bm = Instance.new("Frame")
                            local bn = Instance.new("UIListLayout")
                            local bo = Instance.new("TextLabel")
                            local bp = Instance.new("UIPadding")
                            local bq = Instance.new("UIPadding")
                            local br = Instance.new("Frame")
                            bm.Name = "Section"
                            bm.Parent = aa
                            bm.AutomaticSize = Enum.AutomaticSize.Y
                            bm.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                            bm.BackgroundTransparency = 1.000
                            bm.BorderSizePixel = 0
                            bm.Size = UDim2.new(1, 0, 0, 0)
                            bn.Parent = bm
                            bn.HorizontalAlignment = Enum.HorizontalAlignment.Center
                            bn.SortOrder = Enum.SortOrder.LayoutOrder
                            bn.Padding = UDim.new(0, 5)
                            bo.Name = "Title"
                            bo.Parent = bm
                            bo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                            bo.BackgroundTransparency = 1.000
                            bo.LayoutOrder = -2
                            bo.Size = UDim2.new(1, 0, 0, 7)
                            bo.ZIndex = 5
                            bo.Font = Enum.Font.Gotham
                            bo.Text = sector_cfg.Text
                            bo.TextColor3 = Color3.fromRGB(108, 108, 108)
                            bo.TextSize = 12.000
                            bo.TextXAlignment = Enum.TextXAlignment.Left
                            bo.TextYAlignment = Enum.TextYAlignment.Top
                            bp.Parent = bo
                            bp.PaddingBottom = UDim.new(0, 10)
                            bp.PaddingLeft = UDim.new(0, 15)
                            if sector_cfg.Alignment == "left" then
                                bo.TextXAlignment = Enum.TextXAlignment.Left
                                bp.PaddingRight = UDim.new(0, 0)
                                bp.PaddingLeft = UDim.new(0, 15)
                            elseif sector_cfg.Alignment == "right" then
                                bo.TextXAlignment = Enum.TextXAlignment.Right
                                bp.PaddingRight = UDim.new(0, 15)
                                bp.PaddingLeft = UDim.new(0, 0)
                            elseif sector_cfg.Alignment == "center" then
                                bo.TextXAlignment = Enum.TextXAlignment.Center
                                bp.PaddingRight = UDim.new(0, 0)
                                bp.PaddingLeft = UDim.new(0, 0)
                            end
                            bq.Parent = bm
                            bq.PaddingTop = UDim.new(0, 3)
                            br.Parent = bm
                            br.AnchorPoint = Vector2.new(0, 1)
                            br.BackgroundColor3 = Color3.fromRGB(49, 49, 49)
                            br.BorderSizePixel = 0
                            br.LayoutOrder = -1
                            br.Position = UDim2.new(0.0503597111, 0, 0.144654095, 0)
                            br.Size = UDim2.new(1, -25, 0, 1)
                            local bs = {}
                            function bs:CreateButton(bt)
                                button_cfg = {
                                    Text = bt.Text or "button",
                                    Disabled = bt.Disabled or false,
                                    Warning = bt.Warning or false,
                                    Callback = bt.Callback or function()
                                        end
                                }
                                local bu = Instance.new("TextButton")
                                local bv = Instance.new("UICorner")
                                local bw = Instance.new("TextLabel")
                                local bx = Instance.new("UIPadding")
                                bu.Name = "clickButton"
                                bu.Parent = bm
                                bu.AnchorPoint = Vector2.new(0.5, 0.5)
                                bu.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
                                bu.Size = UDim2.new(1, -20, 0, 34)
                                bu.AutoButtonColor = false
                                bu.Font = Enum.Font.SourceSans
                                bu.Text = ""
                                bu.TextColor3 = Color3.fromRGB(0, 0, 0)
                                bu.TextSize = 14.000
                                bv.CornerRadius = UDim.new(0, 5)
                                bv.Name = "buttonCorner"
                                bv.Parent = bu
                                bw.Name = "buttonText"
                                bw.Parent = bu
                                bw.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                bw.BackgroundTransparency = 1.000
                                bw.Size = UDim2.new(1, 0, 0, 34)
                                bw.Font = Enum.Font.GothamMedium
                                bw.Text = button_cfg.Text
                                bw.TextColor3 = Color3.fromRGB(149, 149, 149)
                                bw.TextSize = 13.000
                                bw.TextXAlignment = Enum.TextXAlignment.Left
                                bx.Name = "buttonTextPadding"
                                bx.Parent = bw
                                bx.PaddingLeft = UDim.new(0, 10)
                                t:Create(
                                    {
                                        Name = "Button Animation",
                                        Speed = 0.14,
                                        Style = Enum.EasingStyle.Sine,
                                        Direction = Enum.EasingDirection.InOut,
                                        Loop = 0,
                                        Reverse = false,
                                        Delay = 0
                                    }
                                )
                                if bt.Disabled == true then
                                    bu.BackgroundTransparency = 0.2
                                    if bt.Warning == true then
                                        bw.TextColor3 = Color3.fromRGB(141, 54, 56)
                                    else
                                        bw.TextColor3 = Color3.fromRGB(100, 100, 100)
                                    end
                                else
                                    bu.BackgroundTransparency = 0
                                    if bt.Warning == true then
                                        bw.TextColor3 = Color3.fromRGB(177, 73, 75)
                                    else
                                        bw.TextColor3 = Color3.fromRGB(149, 149, 149)
                                    end
                                end
                                bu.MouseEnter:Connect(
                                    function()
                                        if bt.Disabled == false then
                                            t:Animate(
                                                bu,
                                                {BackgroundColor3 = Color3.fromRGB(40, 40, 40)},
                                                "Button Animation"
                                            )
                                            if bt.Warning == false then
                                                t:Animate(
                                                    bw,
                                                    {TextColor3 = Color3.fromRGB(180, 180, 180)},
                                                    "Button Animation"
                                                )
                                            else
                                                t:Animate(
                                                    bw,
                                                    {TextColor3 = Color3.fromRGB(177, 73, 75)},
                                                    "Button Animation"
                                                )
                                            end
                                        end
                                    end
                                )
                                bu.MouseLeave:Connect(
                                    function()
                                        if bt.Disabled == false then
                                            t:Animate(
                                                bu,
                                                {BackgroundColor3 = Color3.fromRGB(35, 35, 35)},
                                                "Button Animation"
                                            )
                                            if bt.Warning == false then
                                                t:Animate(
                                                    bw,
                                                    {TextColor3 = Color3.fromRGB(149, 149, 149)},
                                                    "Button Animation"
                                                )
                                            else
                                                t:Animate(
                                                    bw,
                                                    {TextColor3 = Color3.fromRGB(177, 73, 75)},
                                                    "Button Animation"
                                                )
                                            end
                                        end
                                    end
                                )
                                bu.MouseButton1Down:Connect(
                                    function()
                                        if bt.Disabled == false then
                                            t:Animate(
                                                bu,
                                                {BackgroundColor3 = Color3.fromRGB(30, 30, 30)},
                                                "Button Animation"
                                            )
                                        end
                                    end
                                )
                                bu.MouseButton1Up:Connect(
                                    function()
                                        if bt.Disabled == false then
                                            t:Animate(
                                                bu,
                                                {BackgroundColor3 = Color3.fromRGB(35, 35, 35)},
                                                "Button Animation"
                                            )
                                            if bt.Warning == false then
                                                t:Animate(
                                                    bw,
                                                    {TextColor3 = Color3.fromRGB(149, 149, 149)},
                                                    "Button Animation"
                                                )
                                            else
                                                t:Animate(
                                                    bw,
                                                    {TextColor3 = Color3.fromRGB(177, 73, 75)},
                                                    "Button Animation"
                                                )
                                            end
                                        end
                                    end
                                )
                                bu.MouseButton1Click:Connect(
                                    function()
                                        if bt.Disabled == false then
                                            bt.Callback()
                                        end
                                    end
                                )
                                local by = {}
                                function by:ChangeText(bz, bool)
                                    bz = bz or bt.Text
                                    bt.Text = bz
                                    if bool then
                                        t:Animate(bw, {TextTransparency = 1}, "Text Change Animation")
                                        wait(0.12)
                                        bw.Text = bt.Text
                                        wait()
                                        t:Animate(bw, {TextTransparency = 0}, "Text Change Animation")
                                    else
                                        bw.Text = bt.Text
                                    end
                                    return by
                                end
                                function ChangeButtonText(bz, bool)
                                    bz = bz or bt.Text
                                    bt.Text = bz
                                    if bool then
                                        t:Animate(bw, {TextTransparency = 1}, "Text Change Animation")
                                        wait(0.12)
                                        bw.Text = bt.Text
                                        wait()
                                        t:Animate(bw, {TextTransparency = 0}, "Text Change Animation")
                                    else
                                        bw.Text = bt.Text
                                    end
                                end
                                function by:ChangeCallback(bz)
                                    bz = bz or bt.Callback
                                    bt.Callback = bz
                                    return by
                                end
                                function by:AddWarning()
                                    bt.Warning = true
                                    if bt.Disabled == true then
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(141, 54, 56)}, "Button Animation")
                                    else
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(177, 73, 75)}, "Button Animation")
                                    end
                                    return by
                                end
                                function by:RemoveWarning()
                                    bt.Warning = false
                                    if bt.Disabled == true then
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(100, 100, 100)}, "Button Animation")
                                    else
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(149, 149, 149)}, "Button Animation")
                                    end
                                    return by
                                end
                                function AddButtonWarning()
                                    bt.Warning = true
                                    if bt.Disabled == true then
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(141, 54, 56)}, "Button Animation")
                                    else
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(177, 73, 75)}, "Button Animation")
                                    end
                                end
                                function RemoveButtonWarning()
                                    bt.Warning = false
                                    if bt.Disabled == true then
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(100, 100, 100)}, "Button Animation")
                                    else
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(149, 149, 149)}, "Button Animation")
                                    end
                                end
                                function by:Disable()
                                    bt.Disabled = true
                                    t:Animate(bu, {BackgroundTransparency = 0.2}, "Button Animation")
                                    if bt.Warning == true then
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(141, 54, 56)}, "Button Animation")
                                    else
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(100, 100, 100)}, "Button Animation")
                                    end
                                    return by
                                end
                                function by:Enable()
                                    bt.Disabled = false
                                    t:Animate(bu, {BackgroundTransparency = 0}, "Button Animation")
                                    if bt.Warning == true then
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(177, 73, 75)}, "Button Animation")
                                    else
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(149, 149, 149)}, "Button Animation")
                                    end
                                    return by
                                end
                                function DisableButton()
                                    bt.Disabled = true
                                    t:Animate(bu, {BackgroundTransparency = 0.2}, "Button Animation")
                                    if bt.Warning == true then
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(141, 54, 56)}, "Button Animation")
                                    else
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(100, 100, 100)}, "Button Animation")
                                    end
                                end
                                function EnableButton()
                                    bt.Disabled = false
                                    t:Animate(bu, {BackgroundTransparency = 0}, "Button Animation")
                                    if bt.Warning == true then
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(177, 73, 75)}, "Button Animation")
                                    else
                                        t:Animate(bw, {TextColor3 = Color3.fromRGB(149, 149, 149)}, "Button Animation")
                                    end
                                end
                                function by:Hide()
                                    bu.Visible = false
                                    return by
                                end
                                function HideButton()
                                    bu.Visible = false
                                end
                                function by:Show()
                                    bu.Visible = true
                                    return by
                                end
                                function ShowButton()
                                    bu.Visible = true
                                end
                                function by:Delete()
                                    bu:Destroy()
                                    return by
                                end
                                function DeleteButton()
                                    bu:Destroy()
                                end
                                return by
                            end
                            function bs:CreateSwitch(bA)
                                switch_cfg = {
                                    Text = bA.Text or "switch",
