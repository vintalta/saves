                                    bB:Destroy()
                                end
                                return bR
                            end
                            function bs:CreateTextbox(bS)
                                textbox_cfg = {
                                    Text = bS.Text or "textbox",
                                    Inputs = {
                                        Placeholder = bS.Inputs.Placeholder or "Placeholder",
                                        Default = bS.Inputs.Default or ""
                                    },
                                    Auto = bS.Auto or true,
                                    Format = bS.Format or "all",
                                    Disabled = bS.Disabled or false,
                                    Warning = bS.Warning or false,
                                    Callback = bS.Callback or function()
                                        end
                                }
                                local bT = Instance.new("Frame")
                                local bU = Instance.new("UICorner")
                                local bV = Instance.new("TextLabel")
                                local bW = Instance.new("UIPadding")
                                local bX = Instance.new("TextBox")
                                local bY = Instance.new("UIPadding")
                                bT.Name = "Textbox"
                                bT.Parent = bm
                                bT.AnchorPoint = Vector2.new(0.5, 0.5)
                                bT.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
                                bT.BorderSizePixel = 0
                                bT.Position = UDim2.new(0, 144, 0, 0)
                                bT.Size = UDim2.new(1, -20, 0, 34)
                                bU.CornerRadius = UDim.new(0, 5)
                                bU.Name = "textboxCorner"
                                bU.Parent = bT
                                bV.Name = "boxText"
                                bV.Parent = bT
                                bV.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                bV.BackgroundTransparency = 1.000
                                bV.Size = UDim2.new(1, 0, 0, 34)
                                bV.Font = Enum.Font.GothamMedium
                                bV.Text = bS.Text
                                bV.TextColor3 = Color3.fromRGB(149, 149, 149)
                                bV.TextSize = 13.000
                                bV.TextXAlignment = Enum.TextXAlignment.Left
                                bW.Name = "boxTextPadding"
                                bW.Parent = bV
                                bW.PaddingLeft = UDim.new(0, 10)
                                bX.Name = "box"
                                bX.Parent = bT
                                bX.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                bX.BackgroundTransparency = 1.000
                                bX.Size = UDim2.new(1, 0, 1, 0)
                                bX.Font = Enum.Font.Gotham
                                bX.PlaceholderColor3 = Color3.fromRGB(107, 107, 107)
                                bX.PlaceholderText = bS.Inputs.Placeholder
                                bX.Text = bS.Inputs.Default
                                bX.TextColor3 = Color3.fromRGB(171, 171, 171)
                                bX.ClipsDescendants = true
                                bX.TextSize = 13.000
                                bX.TextXAlignment = Enum.TextXAlignment.Right
                                bY.Name = "boxPadding"
                                bY.Parent = bX
                                bY.PaddingRight = UDim.new(0, 10)
                                t:Create(
                                    {
                                        Name = "Box Animation",
                                        Speed = 0.14,
                                        Style = Enum.EasingStyle.Sine,
                                        Direction = Enum.EasingDirection.InOut,
                                        Loop = 0,
                                        Reverse = false,
                                        Delay = 0
                                    }
                                )
                                bX:GetPropertyChangedSignal("Text"):Connect(
                                    function()
                                        if bS.Format == "numbers" then
                                            bX.Text = bX.Text:gsub("%D+", "")
                                        end
                                    end
                                )
                                bX:GetPropertyChangedSignal("Text"):Connect(
                                    function()
                                        if bS.Format == "lower" then
                                            bX.Text = bX.Text:lower()
                                        end
                                    end
                                )
                                bX:GetPropertyChangedSignal("Text"):Connect(
                                    function()
                                        if bS.Format == "upper" then
                                            bX.Text = bX.Text:upper()
                                        end
                                    end
                                )
                                bX:GetPropertyChangedSignal("Text"):Connect(
                                    function()
                                        if bS.Format == "all" or bS.Format == "" then
                                            bX.Text = bX.Text
                                        end
                                    end
                                )
                                if bS.Disabled == true then
                                    bX.Visible = false
                                    bT.BackgroundTransparency = 0.2
                                    if bS.Warning == true then
                                        bV.TextColor3 = Color3.fromRGB(141, 54, 56)
                                    else
                                        bV.TextColor3 = Color3.fromRGB(100, 100, 100)
                                        bX.TextColor3 = Color3.fromRGB(100, 100, 100)
                                    end
                                else
                                    bX.Visible = true
                                    bT.BackgroundTransparency = 0
                                    if bS.Warning == true then
                                        bV.TextColor3 = Color3.fromRGB(177, 73, 75)
                                    else
                                        bV.TextColor3 = Color3.fromRGB(149, 149, 149)
                                        bX.TextColor3 = Color3.fromRGB(171, 171, 171)
                                    end
                                end
                                bT.MouseEnter:Connect(
                                    function()
                                        if bS.Disabled == false then
                                            t:Animate(
                                                bT,
                                                {BackgroundColor3 = Color3.fromRGB(40, 40, 40)},
                                                "Box Animation"
                                            )
                                            if bS.Warning == false then
                                                t:Animate(
                                                    bV,
                                                    {TextColor3 = Color3.fromRGB(180, 180, 180)},
                                                    "Box Animation"
                                                )
                                            else
                                                t:Animate(
                                                    bV,
                                                    {TextColor3 = Color3.fromRGB(177, 73, 75)},
                                                    "Box Animation"
                                                )
                                            end
                                        end
                                    end
                                )
                                bT.MouseLeave:Connect(
                                    function()
                                        if bS.Disabled == false then
                                            t:Animate(
                                                bT,
                                                {BackgroundColor3 = Color3.fromRGB(35, 35, 35)},
                                                "Box Animation"
                                            )
                                            if bS.Warning == false then
                                                t:Animate(
                                                    bV,
                                                    {TextColor3 = Color3.fromRGB(149, 149, 149)},
                                                    "Box Animation"
                                                )
                                            else
                                                t:Animate(
                                                    bV,
                                                    {TextColor3 = Color3.fromRGB(177, 73, 75)},
                                                    "Box Animation"
                                                )
                                            end
                                        end
                                    end
                                )
                                bX.Focused:Connect(
                                    function()
                                        aT = false
                                    end
                                )
                                bX.FocusLost:Connect(
                                    function(bZ)
                                        aT = true
                                        if not bS.Auto then
                                            if bZ then
                                                bS.Callback(bX.Text)
                                            end
                                        else
                                            bS.Callback(bX.Text)
                                        end
                                    end
                                )
                                local b_ = {}
                                function b_:ChangeText(bz, bool)
                                    bz = bz or bS.Text
                                    bS.Text = bz
                                    if bool then
                                        t:Animate(TextBox, {TextTransparency = 1}, "Text Change Animation")
                                        wait(0.12)
                                        TextBox.Text = bS.Text
                                        wait()
                                        t:Animate(TextBox, {TextTransparency = 0}, "Text Change Animation")
                                    else
                                        TextBox.Text = bS.Text
                                    end
                                    return b_
                                end
                                function ChangeBoxText(bz, bool)
                                    bz = bz or bS.Text
                                    bS.Text = bz
                                    if bool then
                                        t:Animate(TextBox, {TextTransparency = 1}, "Text Change Animation")
                                        wait(0.12)
                                        TextBox.Text = bS.Text
                                        wait()
                                        t:Animate(TextBox, {TextTransparency = 0}, "Text Change Animation")
                                    else
                                        TextBox.Text = bS.Text
                                    end
                                end
                                function b_:ChangeInput(bz, bool)
                                    bz = bz or bS.Inputs.Default
                                    bS.Inputs.Default = bz
                                    if bool then
                                        t:Animate(bX, {TextTransparency = 1}, "Text Change Animation")
                                        wait(0.12)
                                        bX.Text = bS.Inputs.Default
                                        wait()
                                        t:Animate(bX, {TextTransparency = 0}, "Text Change Animation")
                                    else
                                        bX.Text = bS.Inputs.Default
                                    end
                                    return b_
                                end
                                function ChangeBoxInput(bz, bool)
                                    bz = bz or bS.Inputs.Default
                                    bS.Inputs.Default = bz
                                    if bool then
                                        t:Animate(bX, {TextTransparency = 1}, "Text Change Animation")
                                        wait(0.12)
                                        bX.Text = bS.Inputs.Default
                                        wait()
                                        t:Animate(bX, {TextTransparency = 0}, "Text Change Animation")
                                    else
                                        bX.Text = bS.Inputs.Default
                                    end
                                end
                                function b_:ChangeCallback(bz)
                                    bz = bz or bS.Callback
                                    bS.Callback = bz
                                    return b_
                                end
