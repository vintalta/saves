                                    function()
                                        if bA.Disabled == false then
                                            t:Animate(
                                                bB,
                                                {BackgroundColor3 = Color3.fromRGB(35, 35, 35)},
                                                "Switch Animation"
                                            )
                                            t:Animate(bL, {Size = UDim2.new(0, 0, 0, 0)}, "Switch Animation")
