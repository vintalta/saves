                                                t:Animate(
                                                    bE,
                                                    {TextColor3 = Color3.fromRGB(149, 149, 149)},
                                                    "Switch Animation"
                                                )
                                            else
                                                t:Animate(
                                                    bE,
                                                    {TextColor3 = Color3.fromRGB(177, 73, 75)},
                                                    "Switch Animation"
                                                )
                                            end
                                        end
                                    end
                                )
                                bN.MouseButton1Down:Connect(
                                    function()
                                        if bA.Disabled == false then
                                            t:Animate(
                                                bB,
                                                {BackgroundColor3 = Color3.fromRGB(30, 30, 30)},
                                                "Switch Animation"
                                            )
                                        end
                                    end
                                )
                                bN.MouseButton1Up:Connect(
                                    function()
                                        if bA.Disabled == false then
                                            t:Animate(
                                                bB,
                                                {BackgroundColor3 = Color3.fromRGB(35, 35, 35)},
                                                "Switch Animation"
                                            )
                                            if bA.Warning == false then
                                                t:Animate(
                                                    bE,
                                                    {TextColor3 = Color3.fromRGB(149, 149, 149)},
                                                    "Switch Animation"
                                                )
                                            else
                                                t:Animate(
                                                    bE,
                                                    {TextColor3 = Color3.fromRGB(177, 73, 75)},
                                                    "Switch Animation"
                                                )
                                            end
                                        end
                                    end
                                )
                                bN.MouseButton1Click:Connect(
                                    function()
                                        if bA.Disabled == false then
                                            bA.State = not bA.State
                                            local bO = bA.State and config.Accent or Color3.fromRGB(50, 50, 50)
                                            if bA.Warning == true then
                                                bO =
                                                    bA.State and Color3.fromRGB(177, 73, 75) or
                                                    Color3.fromRGB(50, 50, 50)
                                            end
                                            local bP = bA.State and UDim2.new(0, 24, 0.5, 0) or UDim2.new(0, 10, 0.5, 0)
                                            local bQ =
                                                bA.State and Color3.fromRGB(170, 170, 170) or Color3.fromRGB(60, 60, 60)
                                            t:Animate(bH, {BackgroundColor3 = bO}, "Switch Animation")
                                            t:Animate(bJ, {Position = bP, BackgroundColor3 = bQ}, "Switch Animation")
                                            bA.Callback(bA.State)
                                        end
                                    end
                                )
                                local bR = {}
                                function bR:ChangeText(bz, bool)
                                    bz = bz or bA.Text
                                    bA.Text = bz
                                    if bool then
                                        t:Animate(bE, {TextTransparency = 1}, "Text Change Animation")
                                        wait(0.12)
                                        bE.Text = bA.Text
                                        wait()
                                        t:Animate(bE, {TextTransparency = 0}, "Text Change Animation")
                                    else
                                        bE.Text = bA.Text
                                    end
                                    return bR
                                end
                                function ChangeSwitchText(bz, bool)
                                    bz = bz or bA.Text
                                    bA.Text = bz
                                    if bool then
                                        t:Animate(bE, {TextTransparency = 1}, "Text Change Animation")
                                        wait(0.12)
                                        bE.Text = bA.Text
                                        wait()
                                        t:Animate(bE, {TextTransparency = 0}, "Text Change Animation")
                                    else
                                        bE.Text = bA.Text
                                    end
                                end
                                function bR:ChangeCallback(bz)
                                    bz = bz or bA.Callback
                                    bA.Callback = bz
                                    return bR
                                end
                                function bR:AddWarning()
                                    bA.Warning = true
                                    if bA.Disabled == true then
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(141, 54, 56)}, "Switch Animation")
                                    else
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(177, 73, 75)}, "Switch Animation")
                                    end
                                    return bR
                                end
                                function bR:RemoveWarning()
                                    bA.Warning = false
                                    if bA.Disabled == true then
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(100, 100, 100)}, "Switch Animation")
                                    else
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(149, 149, 149)}, "Switch Animation")
                                    end
                                    return bR
                                end
                                function AddSwitchWarning()
                                    bA.Warning = true
                                    if bA.Disabled == true then
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(141, 54, 56)}, "Switch Animation")
                                    else
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(177, 73, 75)}, "Switch Animation")
                                    end
                                end
                                function RemoveSwitchWarning()
                                    bA.Warning = false
                                    if bA.Disabled == true then
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(100, 100, 100)}, "Switch Animation")
                                    else
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(149, 149, 149)}, "Switch Animation")
                                    end
                                end
                                function bR:Disable()
                                    bA.Disabled = true
                                    t:Animate(bB, {BackgroundTransparency = 0.2}, "Switch Animation")
                                    if bA.Warning == true then
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(141, 54, 56)}, "Switch Animation")
                                    else
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(100, 100, 100)}, "Switch Animation")
                                    end
                                    return bR
                                end
                                function bR:Enable()
                                    bA.Disabled = false
                                    t:Animate(bB, {BackgroundTransparency = 0}, "Button Animation")
                                    if bA.Warning == true then
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(177, 73, 75)}, "Switch Animation")
                                    else
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(149, 149, 149)}, "Switch Animation")
                                    end
                                    return bR
                                end
                                function DisableSwitch()
                                    bA.Disabled = true
                                    t:Animate(bB, {BackgroundTransparency = 0.2}, "Button Animation")
                                    if bA.Warning == true then
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(141, 54, 56)}, "Switch Animation")
                                    else
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(100, 100, 100)}, "Switch Animation")
                                    end
                                end
                                function EnableSwitch()
                                    bA.Disabled = false
                                    t:Animate(bB, {BackgroundTransparency = 0}, "Switch Animation")
                                    if bA.Warning == true then
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(177, 73, 75)}, "Switch Animation")
                                    else
                                        t:Animate(bE, {TextColor3 = Color3.fromRGB(149, 149, 149)}, "Switch Animation")
                                    end
                                end
