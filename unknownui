                                        b1.TextXAlignment = Enum.TextXAlignment.Right
                                        b2.PaddingRight = UDim.new(0, 15)
                                        b2.PaddingLeft = UDim.new(0, 0)
                                        wait()
                                        t:Animate(b1, {TextTransparency = 0}, "Text Change Animation")
                                    elseif bz == "center" then
                                        t:Animate(b1, {TextTransparency = 1}, "Text Change Animation")
                                        wait(0.12)
                                        b1.TextXAlignment = Enum.TextXAlignment.Center
                                        b2.PaddingRight = UDim.new(0, 0)
                                        b2.PaddingLeft = UDim.new(0, 0)
                                        wait()
                                        t:Animate(b1, {TextTransparency = 0}, "Text Change Animation")
                                    end
                                else
                                    if bz == "left" then
                                        b1.TextXAlignment = Enum.TextXAlignment.Left
                                        b2.PaddingRight = UDim.new(0, 0)
                                        b2.PaddingLeft = UDim.new(0, 15)
                                    elseif bz == "right" then
                                        b1.TextXAlignment = Enum.TextXAlignment.Right
                                        b2.PaddingRight = UDim.new(0, 15)
                                        b2.PaddingLeft = UDim.new(0, 0)
                                    elseif bz == "center" then
                                        b1.TextXAlignment = Enum.TextXAlignment.Center
                                        b2.PaddingRight = UDim.new(0, 0)
                                        b2.PaddingLeft = UDim.new(0, 0)
